<+><!DOCTYPE html>\r\n<html lang=\"en\" xmlns:th=\"http://www.thymeleaf.org\">\r\n<head>\r\n  <meta charset=\"UTF-8\">\r\n  <title>Title</title>\r\n</head>\r\n<body>\r\n<div th:fragment=\"customersidebar\" class=\"sidebar\">\r\n\r\n  <div class=\"nav\">\r\n    <div class=\"nav-item\">\r\n      <img th:src=\"@{/img/customer/dashboard/icon/sidebar/home.png}\" alt=\"Home\" class=\"nav-icon\">\r\n      <a th:href=\"@{/customer/dashboard}\" class=\"nav-text\">Home</a>\r\n    </div>\r\n\r\n    <div class=\"nav-item\">\r\n      <img th:src=\"@{/img/customer/dashboard/icon/sidebar/categories.png}\" alt=\"Categories\" class=\"nav-icon\">\r\n      <a th:href=\"@{/customer/service/display}\" class=\"nav-text\">Categories</a>\r\n    </div>\r\n\r\n    <div class=\"nav-item\">\r\n      <img th:src=\"@{/img/customer/dashboard/icon/sidebar/bookService.png}\" alt=\"Book Service\" class=\"nav-icon\">\r\n      <a th:href=\"@{/customer/dashboard/book-service}\" class=\"nav-text\" onclick=\"\">Book Service</a>\r\n    </div>\r\n\r\n    <div class=\"nav-item\">\r\n      <img th:src=\"@{/img/customer/dashboard/icon/sidebar/trackOrders.png}\" alt=\"Track Orders\" class=\"nav-icon\">\r\n      <a th:href=\"@{/customer/__${session.userid}__/orderHistory}\" class=\"nav-text\">Track Orders</a>\r\n    </div>\r\n\r\n    <div class=\"nav-item\">\r\n      <img th:src=\"@{/img/customer/dashboard/icon/sidebar/managePets.png}\" alt=\"Manage Pets\" class=\"nav-icon\">\r\n      <a th:href=\"@{/customer/pet/list}\" class=\"nav-text\">Manage Pets</a>\r\n    </div>\r\n  </div>\r\n\r\n  <div class=\"side-ad\">\r\n    <a th:href=\"@{/customer/dashboard}\"><img th:src=\"@{/img/customer/dashboard/ad/ad.jpg}\"></a>\r\n  </div>\r\n\r\n  <div class=\"other\">\r\n    <div class=\"other-item\">\r\n      <img th:src=\"@{/img/customer/dashboard/icon/sidebar/account.png}\" alt=\"Account\" class=\"other-icon\">\r\n      <a th:href=\"@{/customer/__${session.userid}__/profile}\" class=\"other-text\">Account</a>\r\n    </div>\r\n\r\n    <div class=\"other-item\">\r\n      <form role=\"form3\" th:action=\"@{/logout}\" method=\"get\">\r\n        <input type=\"submit\" id=\"logout\" value=\"logout\">\r\n      </form>\r\n    </div>\r\n  </div>\r\n</div>\r\n\r\n\r\n\r\n\r\n<div th:fragment=\"customerheader\" class=\"header-container\">\r\n\r\n  <div class=\"header-left\">\r\n    <!--Add Action to Search-->\r\n    <form th:action=\"@{#}\" method=\"post\" class=\"searchbar-container\">\r\n      <select name=\"searchType\" id=\"searchType\" class=\"searchType\">\r\n        <option value=\"All\">All</option>\r\n        <option value=\"Service\">Service</option>\r\n        <option value=\"Groomer\">Groomer</option>\r\n      </select>\r\n\r\n      <div class=\"searchInput\">\r\n        <label th:for=\"search-input\">\r\n          <input class=\"searchInput-2\" type=\"text\" name=\"searchInput\" id=\"search-input\">\r\n        </label>\r\n\r\n      </div>\r\n\r\n\r\n      <button type=\"submit\" class=\"searchSubmit\">Search</button>\r\n    </form>\r\n  </div>\r\n\r\n  <div class=\"header-right\">\r\n    <div class=\"userIcon\">\r\n      <a th:href=\"@{/customer/__${session.userid}__/profile}\" ><img th:src=\"@{/img/customer/dashboard/icon/sidebar/account.png}\" alt=\"Account\" class=\"other-icon\"></a>\r\n    </div>\r\n  </div>\r\n</div>\r\n\r\n<script>\r\n  // Get all sidebar items\r\n  const sidebarItems = document.querySelectorAll('.nav-item');\r\n\r\n  // Loop through each item and add a click event listener\r\n  sidebarItems.forEach(item => {\r\n    item.addEventListener('click', () => {\r\n// Remove the \"active\" class from all items\r\n      sidebarItems.forEach(item => {\r\n        item.classList.remove('active');\r\n      });\r\n      // Add the \"active\" class to the clicked item\r\n      item.classList.add('active');\r\n    });\r\n  });\r\n</script>\r\n</body>\r\n</html>
